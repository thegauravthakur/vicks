    "api",
    "typescript",
    "javascript",
    "deno",
    "node",
    "browser"
  ],
  "main": "dist/vicks.js",
  "module": "dist/vicks.mjs",
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@types/bun": "latest",
    "esbuild": "^0.19.12",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf ./dist && rollup -c --bundleConfigAsCjs",
